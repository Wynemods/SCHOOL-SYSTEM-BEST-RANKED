{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "lib": ["es2015", "dom"],
    "outDir": "./",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "include": [
    "library-system.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
